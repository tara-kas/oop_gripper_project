<?xml version="1.0"?>
<robot name="cylinder_robot">
  <link name="cylinder_link">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.2"/> <!-- radius and length in meters -->
      </geometry>
      <material name="green">
        <color rgba="0 1 0 1"/> <!-- Red color -->
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>
</robot>
